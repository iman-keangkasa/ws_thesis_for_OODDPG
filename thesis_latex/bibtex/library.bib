file = {:home/asl/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Schmuck, Scherer, Zell - 2016 - Hybrid Metric-Topological 3D Occupancy Grid Maps for Large-scale Mapping.pdf:pdf},
issn = {24058963},
journal = {IFAC-PapersOnLine},
keywords = {Autonomous mobile robots,Hybrid maps,Large-scale mapping,Long-term autonomy,Metric-topological maps,SLAM},
