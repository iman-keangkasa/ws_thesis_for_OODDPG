isbn = {1932-6203},
issn = {19326203},
journal = {PLoS ONE},
number = {6},
pages = {3--10},
pmid = {24964089},
title = {{Gliding swifts attain laminar flow over rough wings}},
volume = {9},
year = {2014}
}
