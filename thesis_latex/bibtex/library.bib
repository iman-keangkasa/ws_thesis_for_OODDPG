journal = {Sensors (Switzerland)},
keywords = {Autonomous obstacle avoidance,Dynamic path planning,Dynamic unstructured environment,RRT algorithm,Robotic manipulator,dynamic environment,manipulator},
mendeley-tags = {dynamic environment,manipulator},
number = {2},
title = {{A method on dynamic path planning for robotic manipulator autonomous obstacle avoidance based on an improved RRT algorithm}},
