file = {:home/asl/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Bernreiter et al. - 2021 - PHASER A Robust and Correspondence-Free Global Pointcloud Registration.pdf:pdf},
issn = {23773766},
journal = {IEEE Robotics and Automation Letters},
keywords = {Mapping,fourier,probability and statistical methods,registration,sensor fusion},
mendeley-tags = {fourier,registration},
number = {2},
pages = {855--862},
publisher = {IEEE},
title = {{PHASER: A Robust and Correspondence-Free Global Pointcloud Registration}},
